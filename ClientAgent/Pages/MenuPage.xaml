<Page x:Class="ClientAgent.Pages.MenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ClientAgent.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MenuPage">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <StackPanel Orientation="Horizontal">
                <Button x:Name="ClientBtn" Content="Клиенты" Style="{StaticResource Button}" Click="ClientBtn_Click"></Button>
                <Button x:Name="AgentBtn" Content="Риелторы" Style="{StaticResource Button}" Click="AgentBtn_Click"></Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>
