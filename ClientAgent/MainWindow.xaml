<Window x:Class="ClientAgent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientAgent"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Clients and Agents" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="73*"/>
            <RowDefinition Height="274*"/>
            <RowDefinition Height="92*"/>

        </Grid.RowDefinitions>
        <Rectangle Fill="#FFB5D5CA"/>

       
        <TextBlock x:Name="TitlePage" FontSize="25" HorizontalAlignment="Center" VerticalAlignment="Center" >Магазин продуктов</TextBlock>
        <Button x:Name="BackBtn" HorizontalAlignment="Right" Margin="0,16,100,16"  Click="BackBtn_Click" Style="{StaticResource Button}">Назад</Button>
        <Button x:Name="ExitBtn" HorizontalAlignment="Right" Margin="0,16,10,16" Click="ExitBtn_Click" Style="{StaticResource Button}">Выход</Button>

        <Frame Grid.Row="1" Name="FrameMain" NavigationUIVisibility="Hidden" Grid.RowSpan="2"></Frame>
    </Grid>
</Window>
